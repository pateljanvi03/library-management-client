    "dev": "vite",
    "build": "vite build",
    "build:prod": "vite build --mode prod",
    "lint": "eslint --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "axios": "^1.1.2",
    "chart.js": "^3.9.1",
    "core-js": "^3.6.5",
    "vee-validate": "^3.4.14",
    "vue": "^2.6.11",
    "vue-chartjs": "^4.1.2",
    "vue-js-modal": "^2.0.1",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.4",
    "babel-eslint": "^10.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.9.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "postcss": "^8.4.12",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.1",
    "rollup-plugin-visualizer": "^5.6.0",
    "tailwindcss": "^3.0.23",
    "unplugin-vue-components": "^0.18.4",
    "vite": "^2.8.6",
    "vite-plugin-eslint": "^1.3.0",
    "vite-plugin-vue2": "^1.9.3",
    "vue-template-compiler": "^2.6.14"
  },
  "lint-staged": {
    "**/**.{js,json,vue,ts}": [
